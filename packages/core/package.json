{
  "version": "0.0.0",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "NODE_ENV='development' rollup --config --watch",
    "build": "NODE_ENV='production' rollup --config",
    "test": "vitest **/*.test.tsx",
    "lint": "eslint src/",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "validate": "npm run lint && npm run test && npm run size",
    "clean": "rm -rf dist/"
  },
  "name": "@virtualizable/core",
  "author": "Tiago Ferreira",
  "module": "./dist/index.mjs",
  "size-limit": [
    {
      "path": "./dist/index.cjs",
      "limit": "5 KB"
    },
    {
      "path": "./dist/index.mjs",
      "limit": "5 KB"
    }
  ],
  "peerDependencies": {},
  "devDependencies": {},
  "dependencies": {}
}
